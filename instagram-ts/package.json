{
	"name": "@i7m/instagram-cli",
	"description": "The unofficial CLI and terminal client for Instagram",
	"version": "1.2.0-beta",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/supreme-gg-gg/instagram-cli"
	},
	"keywords": [
		"instagram",
		"cli",
		"terminal",
		"tui",
		"ink",
		"typescript",
		"react",
		"productivity",
		"instagram-api",
		"command-line",
		"messenger",
		"terminal-ui",
		"brainrot"
	],
	"bin": {
		"instagram-cli": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"format": "prettier --write .",
		"build": "tsc",
		"lint-check": "prettier --check . && xo",
		"dev": "tsc --watch",
		"start": "node dist/cli.js",
		"start:mock": "node dist/cli.mock.js",
		"pretest": "tsc",
		"test": "prettier --check . && xo && ava",
		"postinstall": "patch-package"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"emoji-name-map": "^2.0.3",
		"file-type": "^21.0.0",
		"fuse.js": "^7.1.0",
		"ink": "^6.3.1",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"ink-picture": "^1.3.3",
		"ink-testing-library": "^4.0.0",
		"ink-text-input": "^6.0.0",
		"instagram_mqtt": "^1.2.3",
		"instagram-private-api": "^1.46.1",
		"js-yaml": "^4.1.0",
		"meow": "^11.0.0",
		"open": "^10.2.0",
		"pastel": "^4.0.0",
		"patch-package": "^8.0.1",
		"react": "^19.2.0",
		"read-package-up": "^11.0.0",
		"readline": "^1.3.0",
		"sharp": "^0.34.4",
		"unicode-emoji-json": "^0.8.0",
		"zod": "^4.1.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/js-yaml": "^4.0.9",
		"@types/react": "^19.2.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "0.27.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.0.0",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.3",
		"xo": "^0.60.0"
	},
	"ava": {
		"files": [
			"test.tsx"
		],
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off",
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/no-empty-function": "off",
			"react/no-array-index-key": "off",
			"react/require-default-props": "off",
			"unicorn/prevent-abbreviations": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
